    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13"
  },
  "dependencies": {
    "browserify": "^13.1.1",
    "lite-server": "^2.2.2",
    "promise-polyfill": "^6.0.2",
    "whatwg-fetch": "^2.0.1"
  }
}
