{
  "name": "ag_news_app",
  "version": "1.0.0",
  "description": "News App",
  "main": "dist/bundle.js",
  "scripts": {
    "babel-build": "./node_modules/.bin/babel js/app.js --out-dir dist",
    "bundle": "./node_modules/.bin/browserify dist/js/app.js -o dist/bundle.js",
